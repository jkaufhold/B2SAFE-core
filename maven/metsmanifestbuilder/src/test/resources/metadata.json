{
  "@context": { 
    "@vocab": "./conf/EudatControlledVocabulary.jsonld",
    "path": "@id"
  },
  "Structure": [
    {
      "@type": "Entity",
      "path": "_:${dir}/test.txt",
      "type": "Data",
      "format": "miniseed"
    },
    {
      "@type": "Entity",
      "path": "_:${dir}/test3.txt", 
      "type": "Composite",
      "format": "seed"
    },
    {
      "@type": "Entity",
      "path": "_:${dir}/${n}_1.dat",
      "type": "Data",
      "format": "miniseed",
      "isRelatedTo": [
        { "@id": "_:test.txt" },
        { "@id": "_:test3.txt" },
        { "@id": "_:*/meta*_1.txt"}
      ]      
    },    
    {
      "@type": "Entity",
      "path": "_:*meta*",
      "type": "DescriptiveMetaData",
      "format": "dataless",
      "isRelatedTo": [ 
        { "@id": "_:${dir}/test.txt" },
        { "@id": "_:${dir}/test1.txt" },
        { "@id": "_:${dir}/test2.txt" }
      ]
    }
  ]
}
